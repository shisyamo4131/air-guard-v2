{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "~/*": ["./*"],
      "#app": ["./.nuxt/app"],
      "#imports": ["./.nuxt/imports"]
    },
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "preserve",
    "jsxImportSource": "vue"
  },
  "exclude": ["node_modules", ".nuxt", "dist", ".output", "mocks"],
  "include": ["**/*"]
}
